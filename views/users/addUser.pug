extends adminBase.pug

block content
    .container
        h3 Unregistered Users

        table(class='table')
            thead
                tr
                    th Name
                    th ID
                    th make User
                    th make admin
            tbody
                each user in users
                    tr
                        td= user.profileName
                        td= user.profileID
                        td= input( type='checkbox' name='agreement' unchecked)

        if !users.length
            p No Unregistered users found.