extends adminBase.pug

block content
    .container
        h3 Unregistered Users

        table(class='table')
            thead
                tr
                    th Name
                    th ID
                    th make User
                    th make admin
            tbody
                each user in users
                    tr
                        td= user.profileName
                        td= user.profileID
                        td 
                            input(type='radio' name='type' + user.profileID)
                        td
                            input(type='radio' name='type' + user.profileID)
        if !users.length
            p No Unregistered users found.